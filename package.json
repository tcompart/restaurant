{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "Code that fit's in your head",
  "main": "src/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "./migrate-and-start.sh",
    "build": "rimraf dist && ncc build src/app.js -o dist",
    "test": "dotenv -e config/env.LOCAL jest",
    "ci": "start-server-and-test start http://127.0.0.1:3000 test",
    "deploy": "cd docker && docker build .. -f Dockerfile --no-cache -t restaurant",
    "postinstall": "sleep 10 && prisma migrate"
  },
  "keywords": [
    "code",
    "restaurant",
    "nodejs"
  ],
  "author": "Mark Seemann",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.8",
    "@types/uuid": "^9.0.0",
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.1.3",
    "jest": "^29.2.1",
    "jest-junit": "^15.0.0",
    "prisma": "^4.6.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "body-parser": "^1.20.1",
    "dayjs": "^1.11.6",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  }
}
