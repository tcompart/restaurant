{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "Code that fit's in your head",
  "main": "src/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "./migrate-and-start.sh",
    "build": "rimraf ./dist && ncc build ./src/app.js -o ./dist",
    "test": "docker-compose up db -d && npm run test:ci",
    "test:ci": "start-server-and-test start http://localhost:3000 ci",
    "ci": "dotenv -e config/env.LOCAL jest && mv coverage/coverage-final.json coverage/coverage-unit.json",
    "deploy": "npm test && cd docker && docker build .. -f ./Dockerfile  --no-cache -t restaurant"
  },
  "keywords": [
    "code",
    "restaurant",
    "nodejs"
  ],
  "private": true,
  "author": "Mark Seemann",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.8",
    "@types/uuid": "^9.0.0",
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.1.3",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prisma": "^5.5.0",
    "rimraf": "^5.0.5",
    "start-server-and-test": "^2.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "body-parser": "^1.20.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  }
}
