{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "Code that fit's in your head",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "ncc build app.js -o dist",
    "test": "jest",
    "ci": "start-server-and-test http://127.0.0.1:3000",
    "deploy": "docker build . -f Dockerfile -t tcompart/restaurant",
    "docker": "docker run --rm -p 8080:3000 -d tcompart/restaurant",
    "stop": "docker stop $(docker ps -q --filter ancestor=tcompart/restaurant)"
  },
  "keywords": [
    "code",
    "restaurant",
    "nodejs"
  ],
  "author": "Mark Seemann",
  "license": "ISC",
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.1.3",
    "jest": "^29.2.1",
    "start-server-and-test": "^1.14.0"
  }
}
