{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "Code that fit's in your head",
  "main": "app.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "rimraf dist && ncc build app.js -o dist",
    "test": "jest",
    "ci": "start-server-and-test http://127.0.0.1:3000",
    "deploy": "docker build . -f Dockerfile -t tcompart/restaurant",
    "docker": "docker run --rm -p 8080:3000 -d tcompart/restaurant",
    "stop": "docker stop $(docker ps -q --filter ancestor=tcompart/restaurant)"
  },
  "keywords": [
    "code",
    "restaurant",
    "nodejs"
  ],
  "author": "Mark Seemann",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.8",
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.1.3",
    "jest": "^29.2.1",
    "prisma": "^4.5.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0"
  }
}
