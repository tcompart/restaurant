{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "Code that fit's in your head",
  "main": "src/app.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "./start_locally.sh",
    "build": "rimraf dist && ncc build src/app.js -o dist",
    "test": "jest",
    "ci": "start-server-and-test http://127.0.0.1:3000",
    "deploy": "cd docker && docker build .. -f Dockerfile --no-cache -t restaurant",
    "docker": "./run_docker.sh",
    "stop": "./stop_docker.sh",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "code",
    "restaurant",
    "nodejs"
  ],
  "author": "Mark Seemann",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.8",
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.1.3",
    "jest": "^29.2.1",
    "jest-sonar-reporter": "^2.0.0",
    "prisma": "^4.6.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "body-parser": "^1.20.1",
    "dayjs": "^1.11.6",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2"
  }
}
